	/**
	 * verify{{$ ../name/__value__}} - verifies {{$ ../name/__value__}} password value.
	 *
	 * @param passwordString [String][in]: Password string to be verified.
	 *
	 * @return returns true if passwordString matches, otherwise returns false
	 */
	public function verify{{$ ../name/__value__}}($passwordString) {

		return (crypt($passwordString, $this->{{$ ../name/__value__}}Hash) == $this->{{$ ../name/__value__}}Hash);

	}